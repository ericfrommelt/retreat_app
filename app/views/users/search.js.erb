var modal = $('.modal');
var closeButton = $('<button class="close">Close</button>');
var feedModal = $('#feed-modal')
var friends = $('<%= j(render @users) %>')

feedModal.empty();
feedModal.append(closeButton);

<% if @users.count == 0 %>
  feedModal.append('<h2>No Results</h2>');
<% else %>
  feedModal.append(friends);
<% end %>
modal.fadeIn('fast');
